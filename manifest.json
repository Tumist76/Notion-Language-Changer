{
  "manifest_version": 2,
 
  "name": "Notion Russian Language Pack",
  "version": "0.1.4",
  "default_locale": "en",
  "description": "Unofficial Russian language pack for Notion",
  "icons": { 
    "16": "assets/icon16.png",
    "48": "assets/icon48.png",
    "128": "assets/icon128.png" },
 
  "content_scripts": [{
    "js": ["content.js"],
    "matches": ["https://notion.so/*", "https://www.notion.so/*"]
  }]
}